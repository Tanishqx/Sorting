def insertionSort(arr):
    for step in range (1,len(arr)):
        key=arr[step]
        j=step-1
        while j>=0 and key < arr[j]:
            arr[j+1] =arr [j]
            j -= 1 
        arr[j+1] = key
        
input = [4,2,8,3,9,2,4,7,5]
insertionSort(input)
print("Sorted array in Ascending order:", input , end = " ")
            